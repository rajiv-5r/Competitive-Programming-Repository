#include<iostream>
using namespace std;
int main()
{
    char a[100];
    int i;
    cin>>a;
    if((a[0]+a[1])%2!=0)
    cout<<"invalid";
    else if(a[2]=='A'||a[2]=='E'||a[2]=='I'||a[2]=='O'||a[2]=='U'||a[2]=='Y')
    cout<<"invalid";
    else if((a[3]+a[4])%2!=0)
    cout<<"invalid";
    else if((a[4]+a[5])%2!=0)
    cout<<"invalid";
    else if((a[7]+a[8])%2!=0)
    cout<<"invalid";
    else
    cout<<"valid";
    return 0;
    
}
